{
  "name": "snapcraft-build-action",
  "version": "1.0.0",
  "private": true,
  "description": "A Github action that build snapcraft projects",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhenstridge/snapcraft-build-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "James Henstridge",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.22",
    "@typescript-eslint/parser": "^2.13.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.0",
    "eslint-plugin-jest": "^23.2.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  }
}
