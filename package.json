{
  "name": "snapcraft-build-action",
  "version": "1.0.3",
  "private": true,
  "description": "A Github action that build snapcraft projects",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snapcore/action-build.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "James Henstridge",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.3",
    "@actions/exec": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.37",
    "@typescript-eslint/parser": "^2.29.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.4.0",
    "jest-circus": "^25.4.0",
    "js-yaml": "^3.13.1",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
